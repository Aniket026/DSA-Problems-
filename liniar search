
#include <iostream>
using namespace std;
bool liniar(int array[],int n)
{
    
    int number;
    cout<<"enter thr number";
   cin>>number;
  bool isvalid=true;
    for(int i=0;i<n;i++)
    {
        if(number==array[i])
        {
        return 1;
        }
      
    }
  return 0;
}
int main() {
    int number,size;
    int arr[100];
    cin>>size;
    for(int i=0l;i<size;i++){
        cin>>arr[i];
    }
  bool found=liniar(arr,size);
  if(found){
      cout<<"Element found";
  }
  else{
      cout<<"element not found";
  }
    return 0;
}
