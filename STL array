#include<iostream>
#include<array>
using namespace std;
int main(){

    array<int,5> arr={1,2,3,4,5};
    int a=arr.size();
 for(int i=0;i<a;i++){
    cout<<arr[i]<<endl;
 }
 cout<<"Third element of arry "<<arr.at(2)<<endl;
 cout<<"is empty or not "<<arr.empty()<<endl;
  cout<<"front element  "<<arr.front()<<endl;
   cout<<"last element "<<arr.back()<<endl;
    return 0;

}
