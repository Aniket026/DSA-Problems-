
#include <iostream>
#include <climits>
using namespace std;
int main() {

    int number;
    cin>>number;
    int ans;
    while(number!=0){
    int digit;

    digit=number%10;
        if (ans>INT_MAX/10 or ans<INT_MIN/10)
{
    cout<<"0";
}
    ans=ans*10+digit;
    number=number/10;
}
cout<<ans;

    return 0;
}
