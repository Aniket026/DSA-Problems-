#include<iostream>
using namespace std;
int bubble(int arr[],int size){
    for(int i=1;i<size;i++){
        for(int j=0;j<size-i;j++){
            if(arr[j]>arr[j+1]){
            swap(arr[j],arr[j+1]);
            }
        }
    }

}
int display(int arr[],int size)
{
    for(int i=0;i<size;i++){
        cout<<arr[i];
    }
    return 0;
}
int main(){
    
int arr[7]={4,6,1,2,3,9,7};
int a=bubble(arr,7);
 display(arr,7);
cout<<a;
return 0;
}




