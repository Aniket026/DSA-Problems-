

#include<iostream>
#include<queue>
using namespace std;
int main(){
    priority_queue<int> maxi;
    priority_queue<int,vector<int>,greater<int>> mini ;
        maxi.push(1);
        maxi.push(2);
        maxi.push(5);
int n=maxi.size();
for(int i=0;i<n;i++){
    cout<<maxi.top()<<" ";
    maxi.pop();
}
   mini.push(3);
      mini.push(9);
         mini.push(1);
            mini.push(7);
int y=mini.size();
cout<<"mini";
for(int i=0;i<y;i++){
    cout<<mini.top()<<" ";
    mini.pop();
}
cout<<endl;
cout<<"mini is empty"<<mini.empty();
}

output:
5 2 1 mini1 3 7 9 
mini is empty1
