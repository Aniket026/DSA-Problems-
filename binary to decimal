// Online C++ compiler to run C++ program online
#include <iostream>
#include <math.h>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i;
   int ans;
   int digit;
  while(n!=0)
  {
      
      digit=n%10;
      if(digit==1){
      ans=ans+pow(2,i);
      
      }
      i++;
      n=n/10;
      
  }
  cout<<ans;

    return 0;
}
