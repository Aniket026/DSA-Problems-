#include<iostream>
#include<deque>
using namespace std;
int main(){

deque<int>q;

q.push_back(1);

cout<<"size of q is "<<q.size()<<endl;
q.push_back(2);

cout<<"size of q is "<<q.size()<<endl;
q.push_back(3);

cout<<"size of q is "<<q.size()<<endl;
q.push_back(4);

cout<<"size of q is "<<q.size()<<endl;
q.push_back(5);

cout<<"size of q is "<<q.size()<<endl;

cout<<"before erase of q is "<<q.size()<<endl;
q.erase(q.begin(),q.begin()+1);
cout<<"after erase of q is "<<q.size()<<endl;
    return 0;

}


